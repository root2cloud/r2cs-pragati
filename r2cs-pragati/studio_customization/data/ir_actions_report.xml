<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data>
        <!-- Template for CASH Receipt -->
        <template id="bank_payment_receipt_report_copy_1" name="CASH Receipt">
            <t t-call="web.external_layout">
                <div class="page">
                    <div class="oe_structure"/>
                    <div class="row">
                        <div class="col-12">
                            <h2>CASH Receipt</h2>
                            <p>Payment Number:
                                <span t-field="doc.name"/>
                            </p>
                            <p>Date:
                                <span t-field="doc.payment_date"/>
                            </p>
                            <p>Amount:
                                <span t-field="doc.amount"
                                      t-options='{"widget": "monetary", "display_currency": doc.currency_id}'/>
                            </p>
                            <p>Partner:
                                <span t-field="doc.partner_id.name"/>
                            </p>
                            <p>Payment Method:
                                <span t-field="doc.payment_method_id.name"/>
                            </p>
                            <p>Memo:
                                <span t-field="doc.communication"/>
                            </p>
                        </div>
                    </div>
                    <div class="oe_structure"/>
                </div>
            </t>
        </template>

