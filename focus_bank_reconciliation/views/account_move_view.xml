<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_form_brs_inherit" model="ir.ui.view">
        <field name="name">account.move.form.brs.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref']" position="after">
                <field name="payment_narration" string="Narration"/>
                <field name="cheque_number" string="Cheque Number" readonly="1"/>
            </xpath>

        </field>
    </record>
</odoo>